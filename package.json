{
  "name": "aws-lambda-api",
  "version": "1.0.0",
  "description": "AWS Lambda API with Serverless Framework",
  "main": "handler.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "invoke-hello": "sls invoke local -f hello",
